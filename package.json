{
	"name": "alicealexandra.com",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"vercel-build": "bash scripts/fetch-content.sh && vite build",
		"migrate:poems": "npx tsx scripts/migrate-poems.ts",
		"migrate:blog": "npx tsx scripts/migrate-blog.ts",
		"migrate:postcards": "npx tsx scripts/migrate-postcards.ts",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@notionhq/client": "^5.6.0",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-vercel": "^5.10.3",
		"@sveltejs/enhanced-img": "^0.4.4",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@types/eslint": "^9.6.1",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.15.0",
		"gsap": "^3.14.2",
		"marked": "^15.0.12",
		"mdsvex": "^0.12.6",
		"motion": "^12.23.26",
		"prettier": "^3.7.4",
		"prettier-plugin-css-order": "^2.1.2",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte": "^5.46.0",
		"svelte-check": "^4.3.4",
		"svelte-highlight": "^7.9.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.0",
		"vite": "^6.4.1",
		"vitest": "^3.2.4"
	},
	"prepare": "svelte-kit sync",
	"type": "module",
	"packageManager": "pnpm@10.26.0"
}
