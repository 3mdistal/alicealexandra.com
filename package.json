{
	"name": "alicealexandra.com",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"vercel-build": "bash scripts/fetch-content.sh && vite build",
		"migrate:poems": "npx tsx scripts/migrate-poems.ts",
		"migrate:blog": "npx tsx scripts/migrate-blog.ts",
		"migrate:postcards": "npx tsx scripts/migrate-postcards.ts",
		"migrate:local": "npx tsx scripts/migrate-blog.ts --local && npx tsx scripts/migrate-poems.ts --local && npx tsx scripts/migrate-postcards.ts --local",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test --pass-with-no-tests",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.0",
		"@notionhq/client": "^5.6.0",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-vercel": "^6.2.0",
		"@sveltejs/enhanced-img": "^0.9.2",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/eslint": "^9.6.1",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^15.15.0",
		"gsap": "^3.14.2",
		"marked": "^15.0.12",
		"mdsvex": "^0.12.6",
		"motion": "^12.23.26",
		"prettier": "^3.7.4",
		"prettier-plugin-css-order": "^2.1.2",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte": "^5.46.0",
		"svelte-check": "^4.3.5",
		"svelte-highlight": "^7.9.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.0",
		"vite": "^7.3.0",
		"vitest": "^3.2.4"
	},
	"prepare": "svelte-kit sync",
	"type": "module",
	"engines": {
		"node": ">=24.12.0"
	},
	"packageManager": "pnpm@10.26.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"sharp"
		]
	}
}
