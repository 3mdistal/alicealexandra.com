{
	"name": "alicealexandra.com",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"vercel-build": "bash scripts/fetch-content.sh && svelte-kit sync && vite build",
		"setup:content": "bash scripts/setup-content.sh",
		"scrape:builder": "node scripts/scrape-builderio.mjs",
		"preview": "vite preview",
		"test": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"check:a11y": "node scripts/check-a11y-guardrails.mjs",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@aws-sdk/client-s3": "^3.982.0",
		"@eslint/js": "^9.39.0",
		"@notionhq/client": "^5.6.0",
		"@playwright/test": "1.57.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-vercel": "^6.2.0",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/eslint": "^9.6.1",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"fast-xml-parser": "^4.5.1",
		"globals": "^16.5.0",
		"gsap": "^3.14.2",
		"highlight.js": "^11.11.1",
		"marked": "^17.0.1",
		"marked-highlight": "^2.2.3",
		"mdsvex": "^0.12.6",
		"motion": "^12.23.26",
		"node-html-parser": "^7.0.1",
		"prettier": "^3.7.4",
		"prettier-plugin-css-order": "^2.1.2",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte": "^5.46.0",
		"svelte-check": "^4.3.5",
		"svelte-highlight": "^7.9.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.0",
		"vite": "^7.3.0",
		"vitest": "^4.0.16"
	},
	"prepare": "svelte-kit sync",
	"type": "module",
	"packageManager": "pnpm@10.26.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"sharp"
		]
	}
}
