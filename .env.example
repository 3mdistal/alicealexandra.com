# GitHub Token for fetching private content repo
GITHUB_TOKEN=your_github_token_here

# Notion API Configuration (only needed for subscriber functionality)
# Get your API key from: https://www.notion.so/my-integrations
NOTION_KEY=your_notion_api_key_here
SUBSCRIBERS_DB=your_subscribers_database_id_here

# Cloudflare R2 (image migration)
R2_ENDPOINT=https://<accountid>.r2.cloudflarestorage.com
R2_BUCKET=your_bucket_name
R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key
R2_PUBLIC_BASE_URL=https://images.alicealexandra.com
R2_KEY_PREFIX=
